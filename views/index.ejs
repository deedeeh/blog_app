<% include partials/header %>

<h1 class='header'>Welcome to blogs</h1>

<% blogs.forEach(blog => { %>
  <div>
    <h2><%= blog.title %></h2>
    <img class='image' src='<%= blog.image %>'>
    <span><%= blog.created %></span>
    <p><%= blog.body %></p>
  </div>
<% }) %>

<% include partials/footer %>